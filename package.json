      "smallHandsColor",
      "hourMarkersColor",
      "minuteMarkersColor",
      "numbersColor",
      "batteryCircleColor",
      "healthCircleColor",
      "infoCirclesColor",
      "infoLeftBackColor",
      "infoRightBackColor",
      "secondsBackColor",
      "dayInMonthcolor",
      "dailyStepsGoal",
      "weatherApi",
      "weatherProvider",
      "enableWeather",
      "weatherUnits",
      "weatherMinutes",
      "lastTimeWeather",
